<script>
$(document).ready(function()){
	let result = '<c:out value="${result}"/>';
	checkAlert(result);
	function checkAlert(result){
		
		if(result === ''){
			return;
		}
		if(result === "enroll success"){
			alert("등록이 완료되었습니다.");
		}
	}
});
</script>

